  // _____________________________________________
  //
  //          Imports and Class Hierarchy
  // _____________________________________________

  import SI = ClaRa.Basics.Units;

  extends ClaRa.Basics.ControlVolumes.Fundamentals.Geometry.BlockShape;
  extends ClaRa.Basics.ControlVolumes.Fundamentals.Geometry.GenericGeometry_N_cv(
    final volume=width*height*Delta_x,
    final A_heat=[2*(width + height)*Delta_x,Delta_x],
    final A_cross=ones(N_cv)*width*height,
    final N_heat=2,
    final diameter_hyd={4*A_heat[i,1]/A_cross[i] for i in 1:N_cv},
    final Delta_x=ClaRa.Basics.Functions.GenerateGrid({0}, length, N_cv),
    final z= cat(1,
               {(z_out - z_in)/(length)*Delta_x[1]/2 + z_in},
               {((z_out - z_in)/(length)*(sum(Delta_x[k] for k in 1:i - 1) + Delta_x[i]/2) + z_in) for i in 2:N_cv}),
    final Delta_z_in = {sum(Delta_x[1:i]) - Delta_x[i]/2 for i in 1:N_cv});


  // _____________________________________________
  //
  //              Visible Parameters
  // _____________________________________________


  parameter SI.Length height=1 "Height of the component" annotation (Dialog(
      tab="General",
      group="Essential Geometry Definition",
      showStartAttribute=false,
      groupImage="modelica://ClaRa/Resources/Images/ParameterDialog/HollowBlock.png",
      connectorSizing=false));
  parameter SI.Length width=1 "Width of the component" annotation (Dialog(group="Essential Geometry Definition"));
  parameter SI.Length length=1 "Length of the component, flow direction" annotation (Dialog(group="Essential Geometry Definition"));


   annotation (Icon(graphics={Bitmap(
          extent={{-100,-100},{100,100}},
          imageSource=
            "",
          fileName="modelica://ClaRa/figures/Components/HollowBlock.png")}));
end ElectricHeater_N_cv;
